    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "4.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "~1.18.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "cacheDirectory": ".tmp/jestCache",
    "roots": [
      "./src",
      "./test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
