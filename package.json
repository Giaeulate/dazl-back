    "@nestjs/cli": "^9.1.2",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bson": "^4.0.5",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/glob": "^7.1.4",
    "@types/helmet": "0.0.48",
    "@types/jest": "28.1.4",
    "@types/multer": "^1.4.7",
