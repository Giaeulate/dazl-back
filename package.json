    "@socket.io/redis-adapter": "^8.2.1",
    "@socket.io/redis-emitter": "^5.1.0",
    "aws-sdk": "^2.1217.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
